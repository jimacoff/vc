{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": "> 1%",
          "uglify": true
        },
        "useBuiltIns": "usage"
      }
    ],
    "react",
    "stage-2"
  ],
  "plugins": [
    "transform-runtime",
    "add-react-displayname",
    "emotion",
    "transform-decorators-legacy",
    "syntax-dynamic-import",
    [
      "transform-class-properties",
      {
        "spec": true
      }
    ]
  ],
  "env": {
    "production": {
      "presets": ["react-optimize"]
    }
  }
}
